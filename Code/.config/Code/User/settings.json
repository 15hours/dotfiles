{
    "window.titleBarStyle": "custom",
    "keyboard.dispatch": "keyCode",
    "editor.rulers": [
        72,
        79
    ],
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.lineNumbers": "relative",
    "vscode-neovim.neovimInitVimPaths.linux": "~/git/dotfiles/Code/User/init.lua",
    "editor.cursorSurroundingLines": 10,
    "editor.autoClosingBrackets": "never",
    "editor.scrollbar.vertical": "hidden",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "files.autoSave": "afterDelay",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "Monokai Classic Icons",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "sqltools.highlightQuery": false,
    "explorer.confirmDelete": false,
    "c4.diagram.structurizr.enabled": true,
    "c4.diagram.plantuml.enabled": true,
    "c4.diagram.renderer": "structurizr",
    "c4.languageserver.connectiontype": "auto",
    "c4.languageserver.logs.enabled": true,
    "workbench.colorTheme": "GitHub Light",
}
